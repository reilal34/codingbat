public int scoresSpecial(int[] a, int[] b) {
  
  
      int sum=largestScore(a)+largestScore(b);


  return sum;
}

public int largestScore(int [] array){
  
  int max=0;
  
  for(int i=0;i<array.length;i++){
    
    if(array[i]%10==0){
      if(array[i]>max){
        max=array[i];
      }
    }
  }
  
  return max;
}
